{
  "name": "react-telemetry-lib",
  "version": "1.0.0",
  "description": "Cross-platform React telemetry library (web + React Native)",
  "private": false,
  "main": "dist/index.web.js",
  "module": "dist/index.web.js",
  "types": "dist/index.web.d.ts",
  "react-native": "dist/index.native.js",
  "exports": {
    ".": {
      "types": {
        "web": "./dist/index.web.d.ts",
        "react-native": "./dist/index.native.d.ts",
        "default": "./dist/index.web.d.ts"
      },
      "import": {
        "web": "./dist/index.web.js",
        "react-native": "./dist/index.native.js",
        "default": "./dist/index.web.js"
      },
      "require": {
        "web": "./dist/index.web.cjs",
        "react-native": "./dist/index.native.cjs",
        "default": "./dist/index.web.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "react",
    "telemetry",
    "analytics",
    "react-native",
    "library"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-navigation": "^3.0.8",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "react-native-get-random-values": "^1.11.0",
    "uuid": "^13.0.0"
  }
}
